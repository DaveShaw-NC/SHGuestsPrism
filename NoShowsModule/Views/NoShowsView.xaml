<UserControl x:Class="NoShowsModule.Views.NoShowsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NoShowsModule.Views"
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True"
             mc:Ignorable="d" 
             d:DesignHeight="440" d:DesignWidth="400">
    <Grid>
        <Label VerticalAlignment="Top"  HorizontalAlignment="Center" Margin="2" Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="3">
            <TextBlock TextDecorations="Underline" Text="{}" FontWeight="Bold"  FontFamily="MS Reference Sans Serif"
                       Foreground="NavajoWhite"  FontSize="16" FontStyle="Italic"/>
        </Label>
    </Grid>
</UserControl>
